<ScrollView
    android:layout_height="wrap_content" android:layout_width="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:stretchColumns="0,1,2"
        tools:context="com.rperez.ongpersonas.personasatendidas.activities.ConsultaActivity">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/atendida"
                android:textSize="22dp"
                android:textStyle="bold" />

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:id="@+id/atendida" />

        </LinearLayout>
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="@string/hombre" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="@string/mujer" />
        </TableRow>
        <View
            android:layout_height="2px"
            android:background="@android:color/black"></View>
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/nacional" />
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/aten_nacional_hombre"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/aten_nacional_mujer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
        </TableRow>
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/extranjero" />
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/aten_extranjero_hombre"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/aten_extranjero_mujer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
        </TableRow>
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/total" />
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/aten_hombre"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/aten_mujer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
        </TableRow>
        <View
            android:layout_height="2px"
            android:background="@android:color/black"></View>
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/total" />
            <LinearLayout
                android:layout_gravity="right">
                <EditText
                    android:id="@+id/aten_total"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="" />
        </TableRow>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/noatendida"
                android:textSize="22dp"
                android:textStyle="bold" />
            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:id="@+id/noatendida" />
        </LinearLayout>
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="@string/hombre" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="@string/mujer" />
        </TableRow>
        <View
            android:layout_height="2px"
            android:background="@android:color/black"></View>
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/nacional" />
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/noaten_nacional_hombre"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/noaten_nacional_mujer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
        </TableRow>
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/extranjero" />
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/noaten_extranjero_hombre"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/noaten_extranjero_mujer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
        </TableRow>
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/total" />
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/noaten_hombre"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
            <LinearLayout
                android:layout_gravity="center_horizontal">
                <EditText
                    android:id="@+id/noaten_mujer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
        </TableRow>
        <View
            android:layout_height="2px"
            android:background="@android:color/black"></View>
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/total" />
            <LinearLayout
                android:layout_gravity="right">
                <EditText
                    android:id="@+id/noaten_total"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:editable="false"
                    android:text="0" />
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="" />
        </TableRow>
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text=""
                android:id="@+id/lblFecha" />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/retrocede"
                    android:id="@+id/prev"
                    android:clickable="true"
                    android:paddingRight="10px">
                </ImageView>
                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/excel"
                    android:clickable="true"
                    android:id="@+id/excel"
                    android:onClick="exportar"
                    android:paddingRight="10px">
                </ImageView>
                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/avance"
                    android:id="@+id/sig"
                    android:clickable="true"
                    android:paddingRight="10px">
                </ImageView>
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="" />
        </TableRow>
    </TableLayout>
</ScrollView>